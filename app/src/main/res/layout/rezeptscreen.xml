<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/background_rezeptscreen"
        android:layout_width="1190dp"
        android:layout_height="853dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/_790679_simple_wallpapers" />

    <ImageView
        android:id="@+id/rezepticon"
        android:layout_width="99dp"
        android:layout_height="99dp"
        android:layout_marginStart="5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/daco_1775891" />

    <TextView
        android:id="@+id/rezepttext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/aldrich"
        android:text="Rezepte"
        android:textColor="#000000"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/rezepticon"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/but_deutsch"
        android:layout_width="232dp"
        android:layout_height="103dp"
        android:layout_marginStart="200dp"
        android:layout_marginTop="150dp"
        android:backgroundTint="#437C83"
        android:fontFamily="@font/aldrich"
        android:text="Deutsch"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/but_tuerkisch"
        android:layout_width="232dp"
        android:layout_height="103dp"
        android:layout_marginStart="200dp"
        android:layout_marginTop="50dp"
        android:backgroundTint="#437C83"
        android:fontFamily="@font/aldrich"
        android:text="Türkisch"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/but_deutsch" />

    <Button
        android:id="@+id/but_italienisch"
        android:layout_width="232dp"
        android:layout_height="103dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="200dp"
        android:backgroundTint="#437C83"
        android:fontFamily="@font/aldrich"
        android:text="Italienisch"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/but_griechisch" />

    <Button
        android:id="@+id/but_zufall"
        android:layout_width="232dp"
        android:layout_height="103dp"
        android:layout_marginTop="50dp"
        android:backgroundTint="#437C83"
        android:fontFamily="@font/aldrich"
        android:text="Zufällig"
        android:textSize="30sp"
        app:layout_constraintEnd_toStartOf="@+id/but_italienisch"
        app:layout_constraintStart_toEndOf="@+id/but_tuerkisch"
        app:layout_constraintTop_toBottomOf="@+id/but_tuerkisch" />

    <Button
        android:id="@+id/but_griechisch"
        android:layout_width="232dp"
        android:layout_height="103dp"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="200dp"
        android:backgroundTint="#437C83"
        android:fontFamily="@font/aldrich"
        android:text="Griechisch"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="473dp"
        android:layout_height="140dp"
        android:layout_marginTop="90dp"
        app:layout_constraintEnd_toEndOf="@+id/but_deutsch"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/german" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="473dp"
        android:layout_height="140dp"
        android:layout_marginBottom="140dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/but_tuerkisch"
        app:srcCompat="@drawable/turky" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="502dp"
        android:layout_height="168dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="75dp"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/greek" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="473dp"
        android:layout_height="140dp"
        android:layout_marginBottom="140dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/but_italienisch"
        app:srcCompat="@drawable/italy" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="254dp"
        android:layout_height="122dp"
        app:srcCompat="@drawable/fallenderwuerfel"
        tools:layout_editor_absoluteX="439dp"
        tools:layout_editor_absoluteY="508dp" />

</androidx.constraintlayout.widget.ConstraintLayout>